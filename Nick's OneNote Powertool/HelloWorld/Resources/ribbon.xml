<?xml version="1.0" encoding="utf-8" ?>
<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui" loadImage="GetImage">
	<ribbon>
		<tabs>
			<tab id="tabCustom" label="Custom">
				<group id="groupHello" label="Hello">
					<button id="buttonHello" label="Hello World!" size="large" screentip="Press this for a 'Hello World' message"
									onAction="showHello" image="HelloWorld.png"/>
					<button id="buttonTestBinary" label="Test Binary" size="large" screentip="Tests a binary function"
									onAction="showBinary" image="HelloWorld.png"/>
					<button id="buttonTestLocation" label="Test Location" size="large" screentip="Tests locations"
									onAction="showLocation" image="HelloWorld.png"/>
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>